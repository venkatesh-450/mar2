#include <stdio.h>
int fun(int num);
int main()
{
    enum check{no,yes};//no=0 & yes=1
   int num;
   printf("enter num:");
   scanf("%d",&num);
   int result = fun(num);
   if(result==no)
   printf("ODD");
   else
   printf("EVEN");
   }
int fun(int num){
    if(num%2==no)
    return yes;//if it is even it returns yes else return no;
    else
    return no;
}
